{
  "name": "extensions-core-project",
  "private": true,
  "version": "0.0.0",
  "watch": {
    "build": {
      "patterns": [
        "src/**",
        "public/**"
      ],
      "ignore": [
        "node_modules"
      ],
      "extensions": [
        "js",
        "json",
        "lock",
        "tsx",
        "ts",
        "css",
        "scss"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && rollup --config rollup.config.ts -p rollup-plugin-typescript2",
    "preview": "vite preview",
    "watch": "npm-watch"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.181",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@vitejs/plugin-react": "^1.3.0",
    "npm-watch": "^0.11.0",
    "rollup": "^2.70.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.2"
  }
}
